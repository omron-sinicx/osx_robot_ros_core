<?xml version="1.0"?>
<launch>
  
  <param name="robot_description" command="$(find xacro)/xacro --inorder '$(find osx_ur5e)/urdf/ur5e_with_scu_hand.urdf.xacro'" />
  
  <node name="joint_state_publisher" pkg="joint_state_publisher" type="joint_state_publisher" />
  
  <node name="robot_state_publisher" pkg="robot_state_publisher" type="robot_state_publisher" />
  
  <node name="rviz" pkg="rviz" type="rviz" args="-d $(find osx_ur5e)/config/default-display.rviz" required="true" />
</launch>
